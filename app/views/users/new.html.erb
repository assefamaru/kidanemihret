<% provide(:title, "Sign Up") %>

<%= react_component('SignUp', {
  authenticityToken: form_authenticity_token,
  errors_exist: @user.errors.any?,
  errors: @errors,
  user: @params,
  first_name_errors: @user.errors.include?(:first_name),
  last_name_errors: @user.errors.include?(:last_name),
  email_errors: @user.errors.include?(:email),
  password_errors: @user.errors.include?(:password),
  password_confirmation_errors: @user.errors.include?(:password_confirmation)
}) %>
